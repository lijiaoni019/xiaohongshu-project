<view class="binner"></view>

<view class = "container">
  <view data-id="{{item.id}}" bindtap="navigateToShowPage" class="card-product" wx:for ="{{bars}}">
    <image mode="aspectFill" src="{{item.image.path}}" />
    <view class="card-product-infos">
    <view ><h2>{{item.name}}</h2></view>
    <view><p>{{item.title}}</p></view>
    </view>
  </view>
</view>
